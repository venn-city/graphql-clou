{
  "name": "@venncity/base-dao",
  "version": "2.0.3",
  "author": "Venn Engineering",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "rm -rf lib/cjs && tsc",
    "build:esm": "rm -rf lib/esm && tsc -m esnext --outDir lib/esm",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint . --fix  --ext .js,.ts",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --runInBand --coverage",
    "lint:ci": "eslint . --format junit -o $LINT_JUNIT_OUTPUT",
    "generate-opencrud-schema": "node ./scripts/generateOpenCRUDSchema.js"
  },
  "bin": {
    "generate-dao-schema": "./bin/generateDaoSchemaCli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@graphql-codegen/core": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "@venncity/cascade-delete": "^1.2.49",
    "@venncity/graphql-transformers": "^1.6.10",
    "@venncity/opencrud-schema-provider": "^1.4.12",
    "@venncity/prisma-generate-schema": "^1.21.0-0",
    "@venncity/sequelize": "1.0.0",
    "@venncity/sequelize-data-provider": "^1.7.15",
    "@venncity/sequelize-model": "^1.8.16",
    "async": "^3.1.0",
    "dataloader": "^1.4.0",
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "lodash": "^4.17.15",
    "moment-random": "^1.0.5",
    "object-hash": "^2.0.3",
    "pluralize": "^8.0.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.157",
    "@types/pluralize": "^0.0.29",
    "@types/sinon": "^9.0.4",
    "@venncity/auth": "*",
    "@venncity/errors": "*",
    "@venncity/eslint-config": "^3.0.1",
    "@venncity/event-pubsub": "*",
    "@venncity/graphql-pagination-enforce": "*",
    "@venncity/jest-config-server": "^3.4.0",
    "@venncity/nested-config": "*",
    "@venncity/prettier-config-server": "^1.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "sinon": "^7.5.0",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "@venncity/auth": "*",
    "@venncity/errors": "*",
    "@venncity/event-pubsub": "*",
    "@venncity/graphql-pagination-enforce": "*",
    "@venncity/nested-config": "*"
  }
}
