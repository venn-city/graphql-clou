{
  "name": "@venncity/sequelize-model",
  "version": "1.8.7",
  "author": "Venn Engineering",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "rm -rf lib/cjs && tsc",
    "build:esm": "rm -rf lib/esm && tsc -m esnext --outDir lib/esm",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint . --fix  --ext .js,.ts",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --runInBand --coverage",
    "lint:ci": "eslint . --format junit -o $LINT_JUNIT_OUTPUT"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@venncity/clou-utils": "^2.0.1",
    "@venncity/nested-config": "^2.0.0",
    "@venncity/opencrud-schema-provider": "^1.4.6",
    "@venncity/sequelize": "1.0.0",
    "aws-xray-sdk": "^3.0.1",
    "cls-hooked": "^4.2.2",
    "cuid": "^2.1.6",
    "lodash": "^4.17.15",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@venncity/eslint-config": "^3.0.1",
    "@venncity/jest-config-server": "^3.3.4",
    "@venncity/prettier-config-server": "^1.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.1",
    "typescript": "^3.9.6"
  }
}
