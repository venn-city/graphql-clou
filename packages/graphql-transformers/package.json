{
  "name": "@venncity/graphql-transformers",
  "version": "1.7.0",
  "author": "Venn Engineering",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "rm -rf lib/cjs && tsc",
    "build:esm": "rm -rf lib/esm && tsc -m esnext --outDir lib/esm",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint . --fix  --ext .js,.ts",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --runInBand --coverage",
    "lint:ci": "eslint . --format junit -o $LINT_JUNIT_OUTPUT"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@types/jest": "^26.0.4",
    "@venncity/clou-utils": "^2.0.7",
    "@venncity/opencrud-schema-provider": "^1.4.15",
    "sequelize": "^6.13.0",
    "@venncity/sequelize-model": "^1.8.19",
    "async": "^3.2.0",
    "deepdash": "^4.2.17",
    "lodash": "^4.17.15",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@venncity/eslint-config": "^4.0.0",
    "@venncity/jest-config-server": "^3.4.3",
    "@venncity/nested-config": "3.1.15",
    "@venncity/prettier-config-server": "^1.1.7",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "@venncity/nested-config": "*"
  }
}
