{
  "name": "@venncity/opencrud-schema-provider",
  "version": "1.4.9",
  "author": "Venn Engineering",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn insertTestGraphqlFilesToLib",
    "build:cjs": "rm -rf lib/cjs && tsc",
    "build:esm": "rm -rf lib/esm && tsc -m esnext --outDir lib/esm",
    "insertTestGraphqlFilesToLib": "cp -R ../../test lib",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint . --fix  --ext .js,.ts",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --runInBand --coverage",
    "lint:ci": "eslint . --format junit -o $LINT_JUNIT_OUTPUT",
    "generate-opencrud-schema": "node ./scripts/generateOpenCRUDSchema.js",
    "generate-opencrud-schema:test": "node ./scripts/generateOpenCRUDSchema.js ./test/datamodel.graphql ./test/openCRUD.graphql"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@venncity/nested-config": "^2.0.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.6",
    "lodash": "^4.17.15",
    "prisma-datamodel": "^1.34.10"
  },
  "devDependencies": {
    "@venncity/eslint-config": "^3.0.1",
    "@venncity/jest-config-server": "^3.4.0",
    "@venncity/prettier-config-server": "^1.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "prisma-generate-schema": "^1.34.10",
    "typescript": "^3.9.6"
  }
}
