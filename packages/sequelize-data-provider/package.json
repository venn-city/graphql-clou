{
  "name": "@venncity/sequelize-data-provider",
  "version": "1.7.1",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "rm -rf lib/cjs && tsc",
    "build:esm": "rm -rf lib/esm && tsc -m esnext --outDir lib/esm",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint . --fix  --ext .js,.ts",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --runInBand --coverage",
    "lint:ci": "eslint . --format junit -o $LINT_JUNIT_OUTPUT",
    "generate-opencrud-schema": "node ./scripts/generateOpenCRUDSchema.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "@venncity/errors": "^1.6.2",
    "@venncity/graphql-transformers": "^1.6.1",
    "@venncity/nested-config": "^2.0.0",
    "@venncity/opencrud-schema-provider": "^1.4.6",
    "@venncity/sequelize": "1.0.0",
    "@venncity/sequelize-model": "^1.8.7",
    "async": "^3.1.0",
    "cuid": "^2.1.8",
    "deepdash": "^4.2.17",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@venncity/eslint-config": "^3.0.1",
    "@venncity/prettier-config-server": "^1.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "typescript": "^3.9.6"
  }
}
